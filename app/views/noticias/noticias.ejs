<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Noticias</title>
		</head>
	<body>

		<h1>Noticias</h1>

		<table border=1>
			<tr>
				<td>ID</td>
				<td>Titulo</td>
				<td>Noticia</td>
				<td>Resumo</td>
				<td>Autor</td>
				<td>Data Noticia</td>
			</tr>
			<%  for(var i=0; i < noticias.length; i++){	%>

			<tr>
				<td><%= noticias[i].id_noticia %></td>			
				<td><%= noticias[i].titulo %></td>
				<td><%= noticias[i].noticia %></td>				
				<td><%= noticias[i].resumo %></td>
				<td><%= noticias[i].autor %></td>
				<td><%= noticias[i].data_noticia %></td>
			</tr>

			<% } %>
		</table>

	</body>
</html>